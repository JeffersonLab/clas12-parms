TS_CRATE trig1

# TS_BLOCK_LEVEL: read N event at a time
# TS_BUFFER_LEVEL: write N event at a time?

# production: 5 5 15 10
TS_BLOCK_LEVEL   20
TS_BUFFER_LEVEL   8
TS_HOLDOFF   1  5 1
TS_HOLDOFF   2  5 1
TS_HOLDOFF   3 15 1
TS_HOLDOFF   4 10 1


# TS GTP trigger mask - disabled
TS_GTP_INPUT_MASK 0x00000000



# TS FP trigger mask
#
# 0x40 - LTCC
# 0x100 - SVT
# 0x200 - CTOF
# 0x400 - CND
# 0x800 - MVT
# 0x1000 - helicity
#
TS_FP_INPUT_MASK 0x00000040


# First arg:      0-disable, 
#                 1-enable; 
#                 |   Prescale (15-7Hz, 7-3.5kHz, 5-15kHz, 4-30kHz, 3-60kHz)
#                 |   |
TS_RANDOM_TRIGGER 1   5

TS_CRATE end
